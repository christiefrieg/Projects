@model Dashboard.ViewModels.FeedViewModel

<div id="feedsMainForm">
    <div ng-controller="FeedController as fc">
        <h2>Use a local account to log in.</h2>

        <fieldset>
            <legend>Log in Form</legend>
            <ol>
                
            </ol>
            <input type="submit" value="Log in" />
        </fieldset>
        <p>
            @Html.ActionLink("Register", "Register") if you don't have an account.
        </p>
    </div>
</div>
<script>
    var e1 = angular.element(document.getElementById("feedsMainForm"));
    var mController = angular.element(document.getElementById("body"));
    mController.scope().activateView(e1);
</script>